{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/health",
      "dest": "/api/main.py"
    },
    {
      "src": "/test",
      "dest": "/api/main.py"
    },
    {
      "src": "/token",
      "dest": "/api/main.py"
    },
    {
      "src": "/register",
      "dest": "/api/main.py"
    },
    {
      "src": "/dashboard",
      "dest": "/api/main.py"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}